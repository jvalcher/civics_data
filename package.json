  "bugs": {
    "url": "https://github.com/jvalcher/civics_data/issues"
  },
  "homepage": "https://github.com/jvalcher/civics_data#readme",
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.1.0",
    "es-main": "^1.2.0",
    "live-server": "^1.1.0",
    "nodemon": "^2.0.21"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "unirest": "^0.6.0"
  }

{
  "name": "civics_data",
  "version": "1.0.0",
  "description": "Get political representatives for U.S. address from Google Civic Information API",
  "type": "module",
  "scripts": {
    "front": "live-server --host=127.0.0.1 --port=5500 --open=./src/public/",
    "fetch-reps": "pm2 start src/backend/fetchReps/fetchRepsServer.js",
    "fetchRepImg": "cd src/backend/scrapeImgUrl && . venv/bin/activate && pm2 start start.sh --name=fetchRepImgUrl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvalcher/civics_data.git"
  },
